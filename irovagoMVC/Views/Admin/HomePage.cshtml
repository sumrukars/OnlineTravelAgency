@model IEnumerable<irovagoMVC.Models.LogMVCModel>

@{
    ViewBag.Title = "View";
}

<h2>Logs</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.actor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.actorID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.operation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.relatedTable)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.relatedRecordID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.actor)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.actorID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.operation)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.relatedTable)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.relatedRecordID)
    </td>
    <td></td>
</tr>
}

</table>
